<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="350.0" prefWidth="480.0" spacing="20.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="it.unisa.se.calculator.CalculatorController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <HBox prefHeight="331.0" prefWidth="339.0">
      <children>
         <VBox prefHeight="310.0" prefWidth="180.0">
            <children>
               <TextField fx:id="operationField">
                  <VBox.margin>
                     <Insets top="40.0" />
                  </VBox.margin>
               </TextField>
               <Button fx:id="submitButton" alignment="TOP_CENTER" contentDisplay="TOP" mnemonicParsing="false" onAction="#onSubmitButtonClick" prefHeight="25.0" prefWidth="60.0" text="Submit" translateX="120.0">
                  <VBox.margin>
                     <Insets top="5.0" />
                  </VBox.margin></Button>
               <GridPane prefHeight="60.0" prefWidth="160.0">
                 <columnConstraints>
                   <ColumnConstraints hgrow="SOMETIMES" maxWidth="50.0" minWidth="10.0" prefWidth="50.0" />
                   <ColumnConstraints hgrow="SOMETIMES" maxWidth="50.0" minWidth="10.0" prefWidth="50.0" />
                     <ColumnConstraints hgrow="SOMETIMES" maxWidth="50.0" minWidth="10.0" prefWidth="50.0" />
                 </columnConstraints>
                 <rowConstraints>
                   <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                   <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                 </rowConstraints>
                  <children>
                     <Button fx:id="sumButton" alignment="CENTER" mnemonicParsing="false" onAction="#onSumButtonClick" prefHeight="25.0" prefWidth="45.0" text="+" />
                     <Button fx:id="subtractButton" alignment="CENTER" mnemonicParsing="false" onAction="#onSubtractButtonClick" prefHeight="25.0" prefWidth="45.0" text="-" GridPane.columnIndex="1" />
                     <Button fx:id="multiplyButton" alignment="CENTER" mnemonicParsing="false" onAction="#onMultiplyButtonClick" prefHeight="25.0" prefWidth="45.0" text="*" GridPane.rowIndex="1" />
                     <Button fx:id="divideButton" alignment="CENTER" mnemonicParsing="false" onAction="#onDivideButtonCLick" prefHeight="25.0" prefWidth="45.0" text="/" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                     <Button fx:id="inversionSignButton" alignment="CENTER" mnemonicParsing="false" onAction="#onInversionSignButtonClick" prefHeight="25.0" prefWidth="45.0" text="+-" GridPane.columnIndex="2" />
                     <Button fx:id="squareButton" alignment="CENTER" mnemonicParsing="false" onAction="#onSquareButtonClick" prefHeight="25.0" prefWidth="45.0" text="Sqrt" GridPane.columnIndex="2" GridPane.rowIndex="1" />
                  </children>
                  <VBox.margin>
                     <Insets top="45.0" />
                  </VBox.margin>
               </GridPane>
               <GridPane>
                 <columnConstraints>
                   <ColumnConstraints hgrow="SOMETIMES" maxWidth="55.0" minWidth="42.0" prefWidth="50.0" />
                     <ColumnConstraints hgrow="SOMETIMES" maxWidth="78.0" minWidth="44.0" prefWidth="50.0" />
                   <ColumnConstraints hgrow="SOMETIMES" maxWidth="83.0" minWidth="50.0" prefWidth="80.0" />
                 </columnConstraints>
                 <rowConstraints>
                   <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                   <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                 </rowConstraints>
                  <children>
                     <Button mnemonicParsing="false" prefHeight="25.0" prefWidth="45.0" text="Clear" />
                     <Button mnemonicParsing="false" prefHeight="25.0" prefWidth="45.0" text="Drop" GridPane.columnIndex="1" />
                     <Button mnemonicParsing="false" prefWidth="45.0" text="Dup" GridPane.columnIndex="2" />
                     <Button mnemonicParsing="false" prefWidth="45.0" text="Swap" GridPane.rowIndex="1" />
                     <Button mnemonicParsing="false" prefWidth="45.0" text="Over" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                  </children>
                  <VBox.margin>
                     <Insets top="20.0" />
                  </VBox.margin>
               </GridPane>
            </children>
         </VBox>
         <TableView fx:id="tableElements" editable="true" prefHeight="290.0" prefWidth="160.0">
           <columns>
             <TableColumn fx:id="columnElements" prefWidth="160.0" text="Elements" />
           </columns>
            <HBox.margin>
               <Insets bottom="10.0" left="100.0" top="10.0" />
            </HBox.margin>
            <columnResizePolicy>
               <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
            </columnResizePolicy>
         </TableView>
      </children>
   </HBox>
</VBox>
