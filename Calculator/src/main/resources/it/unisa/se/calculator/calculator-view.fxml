<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" prefHeight="350.0" prefWidth="480.0" spacing="20.0" style="-fx-background-color: #1C1C1C;" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="it.unisa.se.calculator.CalculatorController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <HBox prefHeight="331.0" prefWidth="339.0">
      <children>
         <VBox fx:id="calculatorContainer" prefHeight="310.0" prefWidth="218.0" style="-fx-background-color: #1c1c1c;">
            <children>
               <TextField fx:id="operationField" prefHeight="20.0" prefWidth="142.0">
                  <VBox.margin>
                     <Insets top="35.0" />
                  </VBox.margin>
               </TextField>
               <HBox alignment="TOP_RIGHT" prefHeight="23.0" prefWidth="181.0">
                  <children>
                     <Label fx:id="errorLabel" alignment="TOP_LEFT" prefHeight="25.0" prefWidth="119.0" text="Errore presente" textFill="#b24646" />
                     <Button fx:id="submitButton" alignment="TOP_CENTER" contentDisplay="TOP" mnemonicParsing="false" onAction="#onSubmitButtonClick" prefHeight="25.0" prefWidth="60.0" style="-fx-background-color: #D4D4D2;" text="Submit">
                        <font>
                           <Font name="System Bold" size="12.0" />
                        </font></Button>
                  </children>
                  <VBox.margin>
                     <Insets top="10.0" />
                  </VBox.margin>
               </HBox>
               <GridPane alignment="CENTER" prefHeight="132.0" prefWidth="212.0" style="-fx-background-color: #1c1c1c;">
                 <columnConstraints>
                   <ColumnConstraints hgrow="SOMETIMES" maxWidth="50.0" minWidth="10.0" prefWidth="50.0" />
                   <ColumnConstraints hgrow="SOMETIMES" maxWidth="50.0" minWidth="10.0" prefWidth="50.0" />
                     <ColumnConstraints hgrow="SOMETIMES" maxWidth="50.0" minWidth="10.0" prefWidth="50.0" />
                 </columnConstraints>
                 <rowConstraints>
                   <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                   <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                 </rowConstraints>
                  <children>
                     <Button fx:id="sumButton" alignment="CENTER" mnemonicParsing="false" onAction="#onSumButtonClick" prefHeight="32.0" prefWidth="42.0" style="-fx-background-color: #ff9500; -fx-background-radius: 10px;" text="+" textFill="WHITE">
                        <font>
                           <Font size="15.0" />
                        </font></Button>
                     <Button fx:id="subtractButton" alignment="CENTER" mnemonicParsing="false" onAction="#onSubtractButtonClick" prefHeight="32.0" prefWidth="42.0" style="-fx-background-color: #ff9500; -fx-background-radius: 10px;" text="-" textFill="WHITE" GridPane.columnIndex="1">
                        <font>
                           <Font size="15.0" />
                        </font></Button>
                     <Button fx:id="multiplyButton" alignment="CENTER" mnemonicParsing="false" onAction="#onMultiplyButtonClick" prefHeight="32.0" prefWidth="42.0" style="-fx-background-color: #FF9500; -fx-background-radius: 10px;" text="*" textFill="WHITE" GridPane.rowIndex="1">
                        <font>
                           <Font name="System Bold" size="15.0" />
                        </font></Button>
                     <Button fx:id="divideButton" alignment="CENTER" mnemonicParsing="false" onAction="#onDivideButtonCLick" prefHeight="32.0" prefWidth="42.0" style="-fx-background-color: #FF9500; -fx-background-radius: 10px;" text="/" textFill="WHITE" GridPane.columnIndex="1" GridPane.rowIndex="1">
                        <font>
                           <Font size="15.0" />
                        </font></Button>
                     <Button fx:id="inversionSignButton" alignment="CENTER" mnemonicParsing="false" onAction="#onInversionSignButtonClick" prefHeight="32.0" prefWidth="42.0" style="-fx-background-color: #ff9500; -fx-background-radius: 10px;" text="+-" textFill="WHITE" GridPane.columnIndex="2">
                        <font>
                           <Font size="15.0" />
                        </font></Button>
                     <Button fx:id="squareButton" alignment="CENTER" mnemonicParsing="false" onAction="#onSquareButtonClick" prefHeight="32.0" prefWidth="42.0" style="-fx-background-color: #FF9500; -fx-background-radius: 10px;" text="Sqrt" textFill="WHITE" GridPane.columnIndex="2" GridPane.rowIndex="1">
                        <font>
                           <Font name="System Bold" size="11.0" />
                        </font></Button>
                  </children>
                  <VBox.margin>
                     <Insets left="10.0" right="10.0" top="15.0" />
                  </VBox.margin>
                  <padding>
                     <Insets bottom="10.0" top="10.0" />
                  </padding>
               </GridPane>
               <GridPane visible="false">
                 <columnConstraints>
                   <ColumnConstraints hgrow="SOMETIMES" maxWidth="55.0" minWidth="42.0" prefWidth="50.0" />
                     <ColumnConstraints hgrow="SOMETIMES" maxWidth="78.0" minWidth="44.0" prefWidth="50.0" />
                   <ColumnConstraints hgrow="SOMETIMES" maxWidth="83.0" minWidth="50.0" prefWidth="80.0" />
                 </columnConstraints>
                 <rowConstraints>
                   <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                   <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                 </rowConstraints>
                  <children>
                     <Button mnemonicParsing="false" prefHeight="25.0" prefWidth="45.0" text="Clear" />
                     <Button mnemonicParsing="false" prefHeight="25.0" prefWidth="45.0" text="Drop" GridPane.columnIndex="1" />
                     <Button mnemonicParsing="false" prefWidth="45.0" text="Dup" GridPane.columnIndex="2" />
                     <Button mnemonicParsing="false" prefWidth="45.0" text="Swap" GridPane.rowIndex="1" />
                     <Button mnemonicParsing="false" prefWidth="45.0" text="Over" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                  </children>
                  <VBox.margin>
                     <Insets top="20.0" />
                  </VBox.margin>
               </GridPane>
            </children>
            <padding>
               <Insets left="5.0" right="5.0" />
            </padding>
         </VBox>
         <TableView fx:id="tableElements" editable="true" prefHeight="281.0" prefWidth="219.0">
           <columns>
             <TableColumn fx:id="columnElements" prefWidth="160.0" sortable="false" style="-fx-font-weight: bold; -fx-alignment: center;" text="Operands" />
           </columns>
            <HBox.margin>
               <Insets bottom="10.0" left="20.0" top="10.0" />
            </HBox.margin>
            <columnResizePolicy>
               <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
            </columnResizePolicy>
         </TableView>
      </children>
   </HBox>
</VBox>
